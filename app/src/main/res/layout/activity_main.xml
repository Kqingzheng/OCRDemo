<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:orientation="vertical"
        android:baselineAligned="false">
    <EditText
        android:id="@+id/result"
        android:layout_width="match_parent"
        android:textSize="24dp"
        android:layout_height="200dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button
            android:text="拍照搜题"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textSize="24dp"
            android:background="@color/colorWhite"
            android:id="@+id/accurate_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="功能转换"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="24dp"
            android:background="@color/colorWhite"
            android:layout_height="wrap_content"
            android:id="@+id/change_basic_button"
            tools:ignore="HardcodedText"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/mainLinear"
        android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:id="@+id/Linear1"
        >
    <Button
        android:text="AC"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:textSize="24dp"
        android:background="@color/colorWhite"
        android:textColor="@color/colorOrange"
        android:layout_height="match_parent"
        android:id="@+id/ac_basic_button"
        tools:ignore="HardcodedText"/>
    <Button
        android:text="删除"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:background="@color/colorWhite"
        android:textColor="@color/colorOrange"
        android:textSize="24dp"
        android:layout_height="match_parent"
        android:id="@+id/delete_basic_button"
        tools:ignore="HardcodedText"/>
    <Button
        android:text="%"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:background="@color/colorWhite"
        android:textColor="@color/colorOrange"
        android:textSize="24dp"
        android:layout_height="match_parent"
        android:id="@+id/bai_basic_button"
        tools:ignore="HardcodedText"/>
    <Button
        android:text="/"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:background="@color/colorWhite"
        android:textColor="@color/colorOrange"
        android:textSize="24dp"
        android:layout_height="match_parent"
        android:id="@+id/div_basic_button"
        tools:ignore="HardcodedText"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:id="@+id/Linear2"
        >
        <Button
            android:text="7"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/seven_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="8"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/eight_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="9"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/nine_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="*"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textColor="@color/colorOrange"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/mul_basic_button"
            tools:ignore="HardcodedText"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:id="@+id/Linear3"
        >
        <Button
            android:text="4"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/four_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="5"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/five_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="6"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/six_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="-"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textColor="@color/colorOrange"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/sub_basic_button"
            tools:ignore="HardcodedText"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:id="@+id/Linear4"
        >
        <Button
            android:text="1"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/one_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="2"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/two_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="3"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/three_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="+"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textColor="@color/colorOrange"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/add_basic_button"
            tools:ignore="HardcodedText"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:id="@+id/Linear5"
        >
        <Button
            android:text="("
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textColor="@color/colorOrange"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/lbracket_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="0"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/zero_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text=")"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textColor="@color/colorOrange"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/rbracket_basic_button"
            tools:ignore="HardcodedText"/>
        <Button
            android:text="="
            android:layout_width="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite"
            android:textColor="@color/colorOrange"
            android:textSize="24dp"
            android:layout_height="match_parent"
            android:id="@+id/equal_basic_button"
            tools:ignore="HardcodedText"/>
    </LinearLayout>
    </LinearLayout>
</LinearLayout>
